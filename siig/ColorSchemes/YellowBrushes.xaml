<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--visual-->
    <SolidColorBrush x:Key="FormBackgroundBrush" Color="#f9a825"/>
    <!--800-->
    <SolidColorBrush x:Key="ContainerBackgroundBrush" Color="#ffee58"/>
    <!--400-->
    <SolidColorBrush x:Key="ContainerBorderBrush" Color="#ffa000"/>

    <Color x:Key="ContainerBorderBrushAnimTo">#ffa000</Color>
    <!--900-->

    <!--text-->
    <SolidColorBrush x:Key="PlaceholderBrush" Color="#424242"/>

    <SolidColorBrush x:Key="TextBrush" Color="#263238"/>
    <!--50-->

    <!--button-->
    <SolidColorBrush x:Key="HoverColor" Color="#fbc02d"/>
    <!--700-->

    <!--io controls-->
    <SolidColorBrush x:Key="ItemsBorderBrush" Color="#ffeb3b"/>
    <!--500-->
    <SolidColorBrush x:Key="ItemsBackgroundBrush" Color="#fdd835"/>
    <!--600-->
    <SolidColorBrush x:Key="ErrorBrush" Color="#d50000"/>


    <!--combobox-->
    <SolidColorBrush x:Key="ComboBoxDisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="ComboBoxDisabledBackgroundBrush" Color="#eee" />
    <SolidColorBrush x:Key="ComboBoxDisabledBorderBrush" Color="#888" />


</ResourceDictionary>