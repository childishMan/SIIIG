<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--visual-->
    <SolidColorBrush x:Key="FormBackgroundBrush" Color="#2e7d32"/>
    <!--800-->
    <SolidColorBrush x:Key="ContainerBackgroundBrush" Color="#66bb6a"/>
    <!--400-->
    <SolidColorBrush x:Key="ContainerBorderBrush" Color="#1b5e20"/>
    <!--900-->

    <!--text-->
    <SolidColorBrush x:Key="PlaceholderBrush" Color="#90a4ae"/>

    <SolidColorBrush x:Key="TextBrush" Color="#e8f5e9"/>
    <!--50-->

    <!--button-->
    <SolidColorBrush x:Key="HoverColor" Color="#388e3c"/>
    <!--700-->

    <!--io controls-->
    <SolidColorBrush x:Key="ItemsBorderBrush" Color="#4caf50"/>
    <!--500-->
    <SolidColorBrush x:Key="ItemsBackgroundBrush" Color="#43a047"/>
    <!--600-->
    <SolidColorBrush x:Key="ErrorBrush" Color="#d50000"/>


    <!--combobox-->
    <SolidColorBrush x:Key="ComboBoxDisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="ComboBoxDisabledBackgroundBrush" Color="#eee" />
    <SolidColorBrush x:Key="ComboBoxDisabledBorderBrush" Color="#888" />


</ResourceDictionary>