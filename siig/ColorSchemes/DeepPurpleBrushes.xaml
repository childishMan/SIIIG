<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--visual-->
    <SolidColorBrush x:Key="FormBackgroundBrush" Color="#6a1b9a"/>
    <SolidColorBrush x:Key="ContainerBackgroundBrush" Color="#ab47bc"/>
    <SolidColorBrush x:Key="ContainerBorderBrush" Color="#4a148c"/>

    <!--text-->
    <SolidColorBrush x:Key="PlaceholderBrush" Color="#90a4ae"/>

    <SolidColorBrush x:Key="TextBrush" Color="#f3e5f5"/>

    <!--button-->
    <SolidColorBrush x:Key="HoverColor" Color="#7b1fa2"/>

    <!--io controls-->
    <SolidColorBrush x:Key="ItemsBorderBrush" Color="#9c27b0"/>
    <SolidColorBrush x:Key="ItemsBackgroundBrush" Color="#8e24aa"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="#f44336"/>


     <!--combobox-->
    <SolidColorBrush x:Key="ComboBoxDisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="ComboBoxDisabledBackgroundBrush" Color="#eee" />
    <SolidColorBrush x:Key="ComboBoxDisabledBorderBrush" Color="#888" />

</ResourceDictionary>