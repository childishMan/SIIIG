<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--visual-->
    <SolidColorBrush x:Key="FormBackgroundBrush" Color="#c62828"/> <!--800-->
    <SolidColorBrush x:Key="ContainerBackgroundBrush" Color="#ef5350"/> <!--400-->
    <SolidColorBrush x:Key="ContainerBorderBrush" Color="#b71c1c"/> <!--900-->

    <!--text-->
    <SolidColorBrush x:Key="PlaceholderBrush" Color="#90a4ae"/>

    <SolidColorBrush x:Key="TextBrush" Color="#ffebee"/> <!--50-->

    <!--button-->
    <SolidColorBrush x:Key="HoverColor" Color="#d32f2f"/> <!--700-->

    <!--io controls-->
    <SolidColorBrush x:Key="ItemsBorderBrush" Color="#f44336"/> <!--500-->
    <SolidColorBrush x:Key="ItemsBackgroundBrush" Color="#e53935"/> <!--600-->
    <SolidColorBrush x:Key="ErrorBrush" Color="#d50000"/>


    <!--combobox-->
    <SolidColorBrush x:Key="ComboBoxDisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="ComboBoxDisabledBackgroundBrush" Color="#eee" />
    <SolidColorBrush x:Key="ComboBoxDisabledBorderBrush" Color="#888" />

</ResourceDictionary>